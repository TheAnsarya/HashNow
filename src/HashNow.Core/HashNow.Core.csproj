<Project Sdk="Microsoft.NET.Sdk">

<PropertyGroup>
<TargetFramework>net10.0</TargetFramework>
<LangVersion>latest</LangVersion>
<ImplicitUsings>enable</ImplicitUsings>
<Nullable>enable</Nullable>
<TreatWarningsAsErrors>true</TreatWarningsAsErrors>

<AssemblyName>HashNow.Core</AssemblyName>
<RootNamespace>HashNow.Core</RootNamespace>
<Version>1.0.2</Version>
<Description>Core library for computing 58 hash algorithms in parallel</Description>
<Authors>TheAnsarya</Authors>
<Company>TheAnsarya</Company>
<Product>HashNow</Product>
<Copyright>Copyright (c) 2025 TheAnsarya</Copyright>

<GenerateDocumentationFile>true</GenerateDocumentationFile>
<NoWarn>CS1591</NoWarn>
</PropertyGroup>

<ItemGroup>
  <PackageReference Include="Blake3" Version="2.2.0" />
  <PackageReference Include="BouncyCastle.Cryptography" Version="2.6.2" />
  <PackageReference Include="HashDepot" Version="3.2.0" />
  <PackageReference Include="murmurhash" Version="1.0.3" />
  <PackageReference Include="SauceControl.Blake2Fast" Version="2.0.0" />
  <PackageReference Include="System.Data.HashFunction.CityHash" Version="2.0.0" />
  <PackageReference Include="System.Data.HashFunction.MurmurHash" Version="2.0.0" />
  <PackageReference Include="System.Data.HashFunction.SpookyHash" Version="2.0.0" />
  <PackageReference Include="System.IO.Hashing" Version="10.0.2" />
</ItemGroup>

</Project>
